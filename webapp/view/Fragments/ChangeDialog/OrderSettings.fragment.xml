<core:FragmentDefinition 
	xmlns="sap.m" 
	xmlns:core="sap.ui.core"
	xmlns:f="sap.ui.layout.form"
	xmlns:l="sap.ui.layout" 
	>
	
	<VBox class="sapUiSmallMargin" width="50rem">
		<CheckBox text="{i18n>SHOW_TAB}" selected="{mGlobal>/CurrentChangeSettings/Order/ObjectTab}" />
		<CheckBox text="{i18n>SHORT_TEXT}" selected="{mGlobal>/CurrentChangeSettings/Order/ShortText}" />
		<CheckBox text="{i18n>PRIORITY}" selected="{mGlobal>/CurrentChangeSettings/Order/Priority}" />
		<CheckBox text="{i18n>WORK_CENTER}" selected="{mGlobal>/CurrentChangeSettings/Order/MnWkCtr}" />
		<CheckBox text="{i18n>FUNCTIONAL_LOCATION}" selected="{mGlobal>/CurrentChangeSettings/Order/FunctLoc}" />
		<CheckBox text="{i18n>EQUIPMENT}" selected="{mGlobal>/CurrentChangeSettings/Order/Equipment}" />
		<CheckBox text="{i18n>RESPONSIBLE}" selected="{mGlobal>/CurrentChangeSettings/Order/PersonResponsible}" />
		<CheckBox text="{i18n>add-user-status}" selected="{mGlobal>/CurrentChangeSettings/Order/AddUserStatus}" />
		<CheckBox text="{i18n>delete-user-status}" selected="{mGlobal>/CurrentChangeSettings/Order/DeleteUserStatus}" />
		<CheckBox text="{i18n>basic-start}" selected="{mGlobal>/CurrentChangeSettings/Order/StartDate}" />
		<CheckBox text="{i18n>basic-finish}" selected="{mGlobal>/CurrentChangeSettings/Order/FinishDate}" />
		<CheckBox text="{i18n>REVISION}" selected="{mGlobal>/CurrentChangeSettings/Order/Revision}" />
		<CheckBox text="{i18n>syst-cond}" selected="{mGlobal>/CurrentChangeSettings/Order/Systcond}" />
		<CheckBox text="{i18n>release}" selected="{mGlobal>/CurrentChangeSettings/Order/Release}" />
		<CheckBox text="{i18n>tecnical-complete}" selected="{mGlobal>/CurrentChangeSettings/Order/TeCo}" />
		
	</VBox>
	
